{
	"compilerOptions": {
		"esModuleInterop": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"inlineSourceMap": true,
		"lib": ["es2015", "dom", "es2015.promise"],
		"module": "es2015",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"strict": true,
		"target": "es5"
	}
}
